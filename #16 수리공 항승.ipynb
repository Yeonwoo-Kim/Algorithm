    "n,L=map(int,input().split())\n",
    "List=list(map(int,input().split()))\n",
    "List=sorted(List,key=lambda x:x)\n",
    "cnt=0\n",
    "m=0\n",
    "\n",
    "for i in range(n-1):\n",
    "    m+=List[i+1]-List[i]\n",
    "    \n",
    "    if m > L-1:\n",
    "        cnt+=1\n",
    "        m=0\n",
    "    \n",
    "\n",
    "print(cnt+1)\n"
   ]
