   "source": [
    "n=int(input())\n",
    "t_list=[]\n",
    "cnt=0\n",
    "for i in range(n):\n",
    "    meet=list(map(int,input().split()))\n",
    "    t_list.append(meet)\n",
    "\n",
    "t_list=sorted(t_list,key=lambda x:x[0])\n",
    "t_list=sorted(t_list,key=lambda x:x[1])\n",
    "\n",
    "end=0\n",
    "\n",
    "for i,j in t_list:\n",
    "    if(i>=end):\n",
    "        cnt+=1\n",
    "        end=j\n",
    "\n",
    "print(cnt)"
   ]
  }
