    "n=int(input())\n",
    "List=list(map(int,input().split()))\n",
    "k=0\n",
    "cnt=0\n",
    "\n",
    "for data in List:\n",
    "    k=0\n",
    "    if data==1:\n",
    "        continue\n",
    "        \n",
    "    for j in range(2,data):\n",
    "        if data % j == 0:\n",
    "            k+=1\n",
    "            \n",
    "    if k==0:\n",
    "        cnt+=1\n",
    "        \n",
    "print(cnt)"
   ]
  }
