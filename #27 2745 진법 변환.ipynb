    "s,n=map(str,input().split())\n",
    "List=[]\n",
    "result=0\n",
    "for i in s:\n",
    "    if i >='A' and i<='Z':\n",
    "        List.append(ord(i)-55)\n",
    "    else:\n",
    "        List.append(int(i))\n",
    "k=0\n",
    "for i in range(len(List)-1,-1,-1):\n",
    "    result+=List[k]*(int(n)**(i))\n",
    "    k+=1\n",
    "    \n",
    "print(result)"
   ]
  }
