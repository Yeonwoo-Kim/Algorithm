    "def solution(n,a,b):\n",
    "    answer=0\n",
    "    \n",
    "    for i in range(1,n//2+1):\n",
    "        if abs(a-b)==1 and (a+1)//2==(b+1)//2:\n",
    "            answer=i\n",
    "            break\n",
    "        else:\n",
    "            a=(a+1)//2\n",
    "            b=(b+1)//2\n",
    "\n",
    "    return answer"
   ]
  }
