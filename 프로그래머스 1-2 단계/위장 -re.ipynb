    "from collections import defaultdict\n",
    "def solution(clothes):\n",
    "    answer = 1   \n",
    "    List=defaultdict(int)\n",
    "    for a,b in clothes:\n",
    "        List[b]+=1\n",
    "    \n",
    "    for v in List.values():\n",
    "        answer=answer*(v+1)\n",
    "               \n",
    "    return answer-1"
   ]
  }
