    "def solution(n):\n",
    "    answer = 0\n",
    "    for i in range(1,n): #시작\n",
    "        cnt=0\n",
    "        for j in range(i,n+1): #끝\n",
    "            cnt+=j\n",
    "            if cnt==n:\n",
    "                answer+=1\n",
    "                break\n",
    "            if cnt>n:\n",
    "                break\n",
    "            \n",
    "    return answer+1 #자기자신"
   ]
  }
